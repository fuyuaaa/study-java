<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- consumer's application desc, used for tracing dependency relationship (not a matching criterion),
    don't set it same as provider -->
    <dubbo:application desc="demo-consumer"/>

    <!-- use multicast registry center to discover services -->
    <dubbo:registry address="zookeeper://106.14.169.161:2181" check="false"/>

    <!-- generate proxy for top.fuyuaaa.study.netty.the remote services, then demoService can be used in top.fuyuaaa.study.netty.the same way as top.fuyuaaa.study.netty.the
    local regular interface -->
    <!--<dubbo:reference id="demoService" check="false" interface="top.fuyuaaa.api.TestService"/>-->
    <dubbo:reference version="1.0" id="testService" check="false" interface="top.fuyuaaa.api.TestService"/>

</beans>